{
  "name": "sfcc-site-exporter",
  "version": "1.0.0",
  "description": "CLI tool to export SFCC site data using b2c-developer-tooling",
  "type": "module",
  "main": "src/index.js",
  "bin": {
    "sfcc-site-exporter": "./src/index.js"
  },
  "scripts": {
    "start": "node src/index.js",
    "export": "node src/index.js export",
